/*每个页面公共css */
view {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
page {
  color: #333;
  background: #fff;
  font-size: 14px;
  width: 100%;
}
._a {
  text-decoration: none;
  color: inherit;
}
image {
  vertical-align: middle;
}
.wmain {
  max-width: 1200px;
  margin: 0 auto;
}
.width100 {
  max-width: 100%;
}
._h1 {
  font-size: 32px;
}
._h3 {
  font-size: 26px;
}
._h4 {
  font-size: 22px;
}
._h5 {
  font-size: 18px;
}
.font0 {
  font-size: 0;
}
.btn-min {
  background: #ff91ab;
  color: #fff;
  display: inline-block;
  line-height: 30px;
  padding: 0 20px;
  -webkit-transition: all .2s;
  transition: all .2s;
  cursor: pointer;
  border: 1px solid #ff91ab;
}
.btn-min:hover {
    background: #fff;
    color: #ff91ab;
    border: 1px solid #ff91ab;
}
.cursor {
  cursor: pointer;
}
.mt20 {
  margin-top: 20px;
}
